  // ? Preloader Start
  body
    #preloader-active
      .preloader.d-flex.align-items-center.justify-content-center
        .preloader-inner.position-relative
          .preloader-circle
          .preloader-img.pere-text
            img(src='assets/img/logo/loder.png', alt='')
    // Preloader Start
    // Header Start
    .header-area.header-transparent
      .main-header
        .header-bottom.header-sticky
          .container-fluid
            .row.align-items-center
              // Logo
              .col-xl-2.col-lg-2
                .logo
                  a(href='index.html')
                    img(src='assets/img/logo/logo.png', alt='')
              .col-xl-10.col-lg-10
                .menu-wrapper.d-flex.align-items-center.justify-content-end
                  // Main-menu
                  .main-menu.d-none.d-lg-block
                    nav
                      ul#navigation
                        li.active
                          a(href='index.html') Home
                        li
                          a(href='courses.html') Courses
                        li
                          a(href='contact.html') Contact
                        // Button
                        li.button-header.margin-left
                          a.btn(href=' https://forms.gle/uC4kbqAB2UtECD2c8') Join
                        li.button-header
                          a.btn.btn3(href='login.html') Log in
              // Mobile Menu
              .col-12
                .mobile_menu.d-block.d-lg-none
    // Header End
    main
      // ? slider Area Start
      section.slider-area.slider-area2
        .slider-active
          // Single Slider
          .single-slider.slider-height2
            .container
              .row
                .col-xl-8.col-lg-11.col-md-12
                  .hero__caption.hero__caption2
                    h1(data-animation='bounceIn', data-delay='0.2s') Contact us
                    // breadcrumb Start
                    nav(aria-label='breadcrumb')
                      ol.breadcrumb
                        li.breadcrumb-item
                          a(href='index.html') Home
                        li.breadcrumb-item
                          a(href='#') Contact
                    // breadcrumb End
                    // ?  Contact Area start
                    section.contact-section
                      .container
                        section.team-area.fix
                          .container
                            .row.justify-content-center
                              .col-xl-7.col-lg-8
                                .section-tittle.text-center.mb-55
                                  h2 Our Teacher
                            .team-active
                              .single-cat.text-center
                                .cat-icon
                                  img(src='assets/img/gallery/team1.png', alt='')
                                .cat-cap
                                  h5
                                    a(href='') Vishakha Mathur
                                  p
                                    | Well qualified teacher with over 15 years of experience
                                    | -Degree in BA and MA Public Administration
                                    | -Certified NTT Teacher.
                        .row
                          .col-12
                            h2.contact-title Get in Touch
                          .col-lg-8
                            form#contactForm.form-contact.contact_form(action='contact_process.php', method='post', novalidate='novalidate')
                              .row
                                .col-12
                                  .form-group
                                    textarea#message.form-control.w-100(name='message', cols='30', rows='9', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter Message'", placeholder=' Enter Message')
                                .col-sm-6
                                  .form-group
                                    input#name.form-control.valid(name='name', type='text', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter your name'", placeholder='Enter your name')
                                .col-sm-6
                                  .form-group
                                    input#email.form-control.valid(name='email', type='email', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter email address'", placeholder='Email')
                                .col-12
                                  .form-group
                                    input#subject.form-control(name='subject', type='text', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter Subject'", placeholder='Enter Subject')
                              .form-group.mt-3
                                button.button.button-contactForm.boxed-btn(type='submit') Send
                          .col-lg-3.offset-lg-1
                            .media.contact-info
                              span.contact-info__icon
                                i.ti-home
                              .media-body
                                h3 Purva Fountain Square Apartment, Marathalli.
                                p Bangalore-560037
                            .media.contact-info
                              span.contact-info__icon
                                i.ti-tablet
                              .media-body
                                h3 +91 9019641522
                                p Mon to Fri 9am to 6pm
                            .media.contact-info
                              span.contact-info__icon
                                i.ti-email
                              .media-body
                                h3 vishakha.mathur@hotmail.com
                                p Send us your query anytime!
                    // Contact Area End
    footer
      .footer-wrappper.footer-bg
        // Footer Start
        .footer-area.footer-padding
          .container
            .row.justify-content-between
