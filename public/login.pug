  body
    // ? Preloader Start
    #preloader-active
      .preloader.d-flex.align-items-center.justify-content-center
        .preloader-inner.position-relative
          .preloader-circle
          .preloader-img.pere-text
            img(src='assets/img/logo/loder.png', alt='')
    // Preloader Start
    main.login-body(data-vide-bg='assets/img/login-bg.mp4')
      // Login Admin
      form.form-default(action='login-bg.mp4', method='POST')
        .login-form
          // logo-login
          .logo-login
            a(href='index.html')
              img(src='assets/img/logo/loder.png', alt='')
          h2 Login Here
          .form-input
            label(for='name') Email
            input(type='email', name='email')
          .form-input
            label(for='name') Password
            input(type='password', name='password')
          .form-input.pt-30
            input(type='submit', name='submit', value='login')
          // Forget Password
          a.forget(href='#') Forget Password
          // Forget Password
          a.registration(href='register.html') Registration
      // /end login form
    script(src='./assets/js/vendor/modernizr-3.5.0.min.js')
